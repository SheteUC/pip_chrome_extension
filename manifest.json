{
  "manifest_version": 2,
  "name": "My Chrome Extension",
  "version": "1.0",
  "description": "Description of your extension",
  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "My Extension"
  },
  "icons": {
    "16": "images/home.png",
    "48": "images/person.png",
    "128": "images/pic.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}